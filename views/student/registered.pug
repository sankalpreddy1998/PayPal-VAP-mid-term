
html
    head
        title V-Pal
        link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css') 
        link(rel='stylesheet', href='https://fonts.googleapis.com/icon?family=Material+Icons') 
    
    body.grey.lighten-5  
        ul(id="dropdown1" class="dropdown-content")
            li
                a(href="/student/logout") Logout

        div.navbar-fixed
            nav(class="nav-wraper indigo")
                a(class="brand-logo" style="margin-left:30px") V-Pal
                ul.right.hide-on-med-and-down
                    li
                        a.dropdown-trigger(href="#!" data-target="dropdown1")= name
                            i.material-icons.right arrow_drop_down


        ul(class="sidenav sidenav-fixed" style="width:200px;margin-top:64px")
            li.lone(style="margin-top:15px")
                a.waves-effect(href="/student/registered")  
                    i.material-icons person
                    span Registered
            li.lone(style="margin-top:15px")
                a.waves-effect(href="/student/register")  
                    i.material-icons group
                    span Register

        div.container(style="margin-top:0px")
            h4.left(style="margin-left:20px") Registered Course
            


        div.container.center(style="margin-left:200px;margin-top:30px")
            table(style="margin:5%;margin-top:2%;margin-right:5%;background-color:white;width:90%")
                thead
                    tr
                        th Course Name
                        th Course Code
                        th Credits
                        th Slot
                        th Time
                        th Block
                        th Class no
                        th Teacher
                tbody
                    each val,index in mapped_data
                        tr
                            td
                                div
                                    p= val.course.name
                            td
                                div
                                    p= val.course.code
                            td
                                div
                                    p= val.course.credits
                            td
                                div
                                    p= val.slot.name
                            td
                                div
                                    p= val.slot.time
                            td
                                div
                                    p= val.class.block
                            td
                                div
                                    p= val.class.class_number
                            td
                                div
                                    p= val.teacher.name
                    tr.yellow.lighten-5
                        td maximum credits: 27
                        td minimum credits: 16
                        td
                        td
                        td
                        td
                        td registered credits:
                        td= current_credits
                        



        div.container(style="margin-top:0px")
            h4.left(style="margin-left:20px") Time Table

        div.container.center(style="margin-left:200px;margin-top:30px")
            table(style="margin:5%;margin-top:2%;margin-right:5%;background-color:white;width:90%")
                thead
                    tr.blue
                        th.grey(style="border:1px solid black;border-radius:0px;")
                        th(style="border:1px solid black;border-radius:0px;") 8:00 AM to 8:50 AM
                        th(style="border:1px solid black;border-radius:0px;") 9:00 AM to 9:50 AM
                        th(style="border:1px solid black;border-radius:0px;") 10:00 AM to 10:50 AM
                        th(style="border:1px solid black;border-radius:0px;") 11:00 AM to 11:50 AM
                        th(style="border:1px solid black;border-radius:0px;") 12:00 AM to 12:50 AM
                        th.grey(style="border:1px solid black;border-radius:0px;")
                        th(style="border:1px solid black;border-radius:0px;") 2:00 AM to 2:50 AM
                        th(style="border:1px solid black;border-radius:0px;") 3:00 AM to 3:50 AM
                        th(style="border:1px solid black;border-radius:0px;") 4:00 AM to 4:50 AM
                        th(style="border:1px solid black;border-radius:0px;") 5:00 AM to 5:50 AM
                        th(style="border:1px solid black;border-radius:0px;") 6:00 AM to 6:50 AM
                tbody
                    each val,index in time_table
                        tr
                            td.grey(style="border:1px solid black;border-radius:0px;font-weight:bold;width:70px")= val[0][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[1][1])= val[1][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[2][1])= val[2][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[3][1])= val[3][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[4][1])= val[4][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[5][1])= val[5][0]
                            td.grey(style="border:1px solid black;border-radius:0px;font-weight:bold;width:70px")= val[6][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[7][1])= val[7][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[8][1])= val[8][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[9][1])= val[9][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[10][1])= val[10][0]
                            td.yellow(style="border:1px solid black;border-radius:0px;" class=val[11][1])= val[11][0]
                            
 
            
            
        script(src="https://code.jquery.com/jquery-3.4.1.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous")
        script(src='https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js')
        script(src='/js/index.js')